{
  "name": "tksq",
  "version": "1.4.0",
  "description": "Universal LLM token compression MCP server. Squeeze your tokens.",
  "type": "module",
  "bin": {
    "tksq": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",

    "lint": "tsc --noEmit",
    "start": "node dist/index.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "llm",
    "token",
    "compression",
    "prompt",
    "claude",
    "gpt",
    "ai",
    "model-context-protocol"
  ],
  "author": "Nexonium",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nexonium/tksq.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "js-tiktoken": "^1.0.18",
    "diff": "^7.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/diff": "^7.0.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
